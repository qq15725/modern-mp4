{
  "name": "modern-mp4",
  "type": "module",
  "version": "0.0.3",
  "packageManager": "pnpm@8.2.0",
  "description": "Encode and decode mp4 frames in the browser, streaming",
  "author": "wxm",
  "license": "MIT",
  "homepage": "https://github.com/qq15725/modern-mp4",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qq15725/modern-mp4.git"
  },
  "bugs": {
    "url": "https://github.com/qq15725/modern-mp4/issues"
  },
  "keywords": [
    "webcodec",
    "mp4",
    "video"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "types/index.d.ts",
  "browser": "dist/index.js",
  "typesVersions": {
    "*": {
      "*": [
        "./types/*",
        "./types/index.d.ts"
      ]
    }
  },
  "files": [
    "mp4box.d.ts",
    "dist",
    "types"
  ],
  "scripts": {
    "dev": "vite docs",
    "lint": "eslint src",
    "test": "vitest --no-threads --no-isolate",
    "build": "vite build && tsc --project tsconfig.build.json",
    "build:docs": "vite build docs",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "release": "bumpp package.json --commit \"release: v%s\" --push --all --tag"
  },
  "devDependencies": {
    "@qq15725/eslint-config": "^1.1.0",
    "@types/dom-webcodecs": "^0.1.8",
    "@types/node": "^20.8.6",
    "bumpp": "^9.2.0",
    "conventional-changelog-cli": "^4.1.0",
    "eslint": "^8.51.0",
    "mp4box": "^0.5.2",
    "typescript": "^4.9.5",
    "vite": "^4.4.11",
    "vitest": "^0.34.6"
  },
  "peerDependencies": {
    "mp4box": "*"
  }
}
